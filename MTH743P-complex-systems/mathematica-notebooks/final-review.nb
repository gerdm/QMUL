Cell[TextData[{
 "Assuming a given f(x) and g(x) = x, transverse stability for the \
one-dimensional system reduces to \n\t\t\t\t\t\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"\[CapitalGamma]", "(", 
         RowBox[{"\[Sigma]", " ", 
          SuperscriptBox["\[Lambda]", 
           RowBox[{"(", "v", ")"}]]}], ")"}], " ", "=", " ", 
        RowBox[{
         RowBox[{"f", "'"}], 
         RowBox[{"(", "x"}]}]}], "*)"}], " ", "+", " ", 
      RowBox[{"\[Sigma]", " ", 
       RowBox[{"Re", "(", " ", 
        SuperscriptBox["\[Lambda]", 
         RowBox[{"(", "v", ")"}]], ")"}], " ", 
       RowBox[{"g", "'"}], " ", 
       RowBox[{"(", "z"}]}]}], "*)"}], " ", ".", " "}], TraditionalForm]],
  TextAlignment->Right,
  FormatType->TraditionalForm,
  FontSize->16,ExpressionUUID->"eaadff64-7e6c-4adb-ad62-e1c022bac056"],
 "For v=1,2,3,4"
}], "Text",
 CellChangeTimes->{{3.830045173772126*^9, 3.830045248685725*^9}, {
  3.8300452807490263`*^9, 
  3.8300453688280277`*^9}},ExpressionUUID->"49b1ab03-90a8-49d9-9237-\
0cfa9378d321"],

